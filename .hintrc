{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "text-align: -webkit-match-parent",
          "scroll-behavior",
          "-webkit-tap-highlight-color",
          "-moz-backface-visibility",
          "text-decoration-skip-ink"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}